#laws-wrapper .laws-admin > ul
#tags-wrapper .tags-admin > ul
  list-style none
  margin 0px
  padding 0px

  li
    height 35px
    line-height 35px
    overflow hidden
    position relative
    margin 10px 0px
    padding 0 0 0px 5px
    border-bottom 1px solid #eee

    &:last-child
      border-bottom-width 0
    a
      display block
      width 100%
      height 100%
      margin 0
      color #333
      font-weight normal

      &:hover
        font-weight bold
        color #428BCA